# AI-Nexus Engine Environment Configuration
# Copy to .env and fill in your values

# Blockchain RPC URLs
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_PROJECT_ID
BSC_RPC_URL=https://bsc-dataseed.binance.org/
POLYGON_RPC_URL=https://polygon-rpc.com/

# Exchange API Keys (set in Render environment variables)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_here
COINBASE_API_KEY=your_coinbase_api_key_here
COINBASE_SECRET_KEY=your_coinbase_secret_here

# Engine Configuration
ENGINE_MODE=production
DAILY_TARGET=50000
MAX_CAPACITY=10000000
RISK_PROFILE=balanced

# Security (set in Render environment variables)
ADMIN_WALLET=your_admin_wallet_address_here
ENCRYPTION_KEY=your_encryption_key_here

# No KYC Mode
NO_KYC_MODE=true
